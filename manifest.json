{ 
    "manifest_version": 3,
    "name": "VintedSorter",
    "version": "1.0", 
    "permissions": ["scripting", "tabs"],
    "description": "Trie les habits par like",
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon16.png",
            "48": "icon48.png",
            "128": "icon128.png"
        }
    },
    "content_scripts": [
    {
        "matches": [
        "*://www.vinted.fr/*",
        "*://www.vinted.de/*",
        "*://www.vinted.es/*",
        "*://www.vinted.it/*",
        "*://www.vinted.be/*",
        "*://www.vinted.nl/*",
        "*://www.vinted.at/*",
        "*://www.vinted.ch/*",
        "*://www.vinted.cz/*",
        "*://www.vinted.dk/*",
        "*://www.vinted.fi/*",
        "*://www.vinted.hu/*",
        "*://www.vinted.lt/*",
        "*://www.vinted.lv/*",
        "*://www.vinted.lu/*",
        "*://www.vinted.pl/*",
        "*://www.vinted.pt/*",
        "*://www.vinted.ro/*",
        "*://www.vinted.se/*",
        "*://www.vinted.sk/*",
        "*://www.vinted.si/*",
        "*://www.vinted.com/*"
        ],
        "js": ["content.js"]
    }
    ]
}
